{
  "default_profiles": [
    {
      "connected_output_names": [
        "DP-2"
      ],
      "profile_name": "single"
    }
  ],
  "profiles": {
    "single": {
      "pactl_sink_option_regexes": [
        ".*\\.analog-stereo",
        ".*\\.usb-.*[Mm]icrophone.*\\.[^.]+-stereo"
      ],
      "outputs": {
        "DP-2": {
          "configuration": {
            "mode": {
              "resolution": {
                "width": 1920,
                "height": 1080
              },
              "refresh_rate": "60.00"
            },
            "position": {
              "x": 0,
              "y": 0
            }
          },
          "primary": true
        }
      }
    },
    "office": {
      "pactl_sink_option_regexes": [
        ".*\\.hdmi-stereo",
        ".*\\.usb-.*[Mm]icrophone.*\\.[^.]+-stereo",
        ".*\\.analog-stereo"
      ],
      "outputs": {
        "HDMI-0": {
          "configuration": {
            "mode": {
              "resolution": {
                "width": 1920,
                "height": 1080
              },
              "refresh_rate": "60.00"
            },
            "position": {
              "x": 0,
              "y": 0
            }
          },
          "primary": true
        },
        "DP-2": {
          "configuration": {
            "mode": {
              "resolution": {
                "width": 1920,
                "height": 1080
              },
              "refresh_rate": "60.00"
            },
            "position": {
              "x": 1920,
              "y": 0
            }
          },
          "primary": false
        }
      }
    }
  }
}
